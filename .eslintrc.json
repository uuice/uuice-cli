{
  "root": true,
  "ignorePatterns": [
    "projects/**/*",
    "bin/uuice"
  ],
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint-config-standard",
    "plugin:prettier/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "no-undef": "off",
    "no-useless-constructor": "off",
    "@typescript-eslint/no-this-alias": "off"
  }
}
